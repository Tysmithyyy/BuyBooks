@{
}
@model Library

<tr>
    <td>@Model.BookId</td>
    <td>@Model.Title</td>
    <td>@Model.AuthorFirstName @Model.AuthorLastName</td>
    <td>@Model.Publisher</td>
    <td>@Model.ISBN</td>
    <td>@Model.Class</td>
    <td>@Model.Category</td>
    <td>@Model.PageCount</td>
    <td>$@Model.Price</td>
</tr>